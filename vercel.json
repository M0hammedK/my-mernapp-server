{
    "installCommand": "npm inistall --legacy-peer-deps",
    "version": 2,
    "builds":[
        {
            "src": "index.js",
            "use": "@vervel/node"
        },{
            "src": "src/**/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {"src": "/(.*)", "dest": "/"}
    ]
}